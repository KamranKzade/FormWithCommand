             xmlns:local="clr-namespace:FormWithCommand"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
               
                <ResourceDictionary Source="Styles\ButtonStyle.xaml"/>
                <ResourceDictionary Source="Styles\LabelTitleStyle.xaml"/>
                <ResourceDictionary Source="Styles\SimpleLabelStyle.xaml"/>
                <ResourceDictionary Source="Styles\ImageStyle.xaml"/>
                <ResourceDictionary Source="Styles\TextBoxStyle.xaml"/>
                <ResourceDictionary Source="Styles\BorderStyle.xaml"/>
            
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
